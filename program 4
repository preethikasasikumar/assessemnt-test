import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int num = scanner.nextInt();
		
		int temp= 0;
		while(num>0){
		    int rem = num % 10;
		    temp = temp *10 + rem;
		    num= num /10;
		}
		System.out.println(temp);
	}
}
